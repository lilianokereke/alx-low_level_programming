#include "lists.h"
/**
 *
 *
 */
listint_t *get_nodeint_at_index(listint_t *head, unsigned int index)
{
	listint_t *temp;
	unsigned int count = 0;

	if (head == NULL)
	{
		return (NULL);
	}
	else
	{	temp = head;
		while (temp != NULL)
		{
			count++;
		}
		temp = temp->next;

		while (index < count)
		{
			temp = temp->next;
		}
		return (temp);
	}
}
